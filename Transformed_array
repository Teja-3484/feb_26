class Solution:
    def constructTransformedArray(self, nums: List[int]) -> List[int]:
        ans=[]
        n=len(nums)
        for i in range(len(nums)):
            if nums[i]<0:
                ans.append(nums[(i+nums[i])%n])
            else:
                ans.append(nums[(i+nums[i])%n])
        return ans
